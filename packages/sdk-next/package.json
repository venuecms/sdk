    "react": ">=19"
  },
  "devDependencies": {
    "@venuecms/sdk": "workspace:^",
    "clsx": "^2.1.1",
    "markdown-to-jsx": "^7.7.3",
    "next": "^15.1.6",
    "react": "^19.0.0",
    "remove-markdown": "^0.6.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.3",
    "tsup": "^8.3.5",
    "typedoc": "^0.27.1",
    "typedoc-plugin-markdown": "^4.3.3",
    "typescript": "^5.5.0",
    "@types/react": "^19.0.8"
